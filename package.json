{
  "name": "atomic-ui",
  "version": "0.0.0",
  "author": "COTAHA",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:angular-scrollable-table": "node dist/angular-scrollable-table/server/server.mjs",
    "lint": "ng lint",
    "deploy": "ng build --configuration production && npx angular-cli-ghpages --dir=dist/angular-scrollable-table/browser",
    "storybook": "ng run angular-scrollable-table:storybook",
    "build-storybook": "ng run angular-scrollable-table:build-storybook"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.9",
    "@fontsource/open-sans": "^5.2.7",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2003.0",
    "@angular-devkit/build-angular": "^20.3.0",
    "@angular-devkit/core": "^20.3.0",
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.0",
    "@compodoc/compodoc": "^1.1.32",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/addon-onboarding": "^10.1.9",
    "@storybook/angular": "^10.1.9",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "21.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "^10.1.9",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "postcss": "^8.5.6",
    "storybook": "^10.1.9",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4"
  }
}
